{
    "name": "trillo",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "watch": "sass --watch scss/main.scss:css/style.css",
        "compile": "sass scss/main.scss css/style.css",
        "prefix": "postcss css/style.css --use autoprefixer --output css/style.prefix.css",
        "compress": "sass css/style.prefix.css css/style.css --style compressed",
        "build": "npm-run-all compile prefix compress"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "autoprefixer": "^10.2.6",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^8.3.1",
        "sass": "^1.34.1"
    }
}